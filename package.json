{
    "name": "postman-to-k6",
    "version": "1.5.0",
    "description": "Convert a Postman collection to k6 JavaScript",
    "keywords": [
        "k6",
        "postman",
        "loadtest",
        "load-test",
        "load-testing",
        "server-load",
        "performance"
    ],
    "homepage": "https://github.com/loadimpact/postman-to-k6",
    "bugs": "https://github.com/loadimpact/postman-to-k6/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/loadimpact/postman-to-k6.git"
    },
    "license": "Apache-2.0",
    "author": "",
    "main": "bin/postman-to-k6.js",
    "bin": {
        "postman-to-k6": "bin/postman-to-k6.js"
    },
    "directories": {
        "bin": "bin",
        "lib": "lib"
    },
    "scripts": {
        "bundle": "node scripts/bundle.js",
        "lint:fix": "yarn lint --fix",
        "lint": "eslint .",
        "format": "prettier -c ./lib/**/*.js ./test/**/*.js",
        "format:fix": "yarn format --write",
        "postinstall": "node scripts/bundle.js",
        "test": "npm-run-all test-unit test-com test-int",
        "test-com": "cross-env NODE_PATH=lib:test ava test/com/**/*.js",
        "test-hoc": "cross-env NODE_PATH=lib:test ava",
        "test-int": "cross-env NODE_PATH=lib:test ava test/int/*.js",
        "test-unit": "cross-env NODE_PATH=lib:test ava test/unit/**/*.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint && yarn test",
            "pre-push": "yarn test"
        }
    },
    "dependencies": {
        "ajv": "^6.12.6",
        "aws4": "^1.9.0",
        "browserify": "^16.5.2",
        "chai": "^4.2.0",
        "cheerio": "1.0.0-rc.6",
        "commander": "^2.20.3",
        "crypto-js": "^3.1.9-1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^6.11.0",
        "fs-extra": "^8.1.0",
        "husky": "^4.2.5",
        "lodash": "^4.17.21",
        "oauth-1.0a": "^2.2.5",
        "papaparse": "^5.1.0",
        "pkginfo": "^0.4.1",
        "postman-collection": "^4.1.0",
        "postman-collection-transformer": "^4.1.3",
        "prettier": "^1.19.1",
        "spo-gpo": "^1.0.0",
        "strip-json-comments": "^3.1.1",
        "urijs": "^1.19.7",
        "uuid": "^3.3.2",
        "verror": "^1.10.0",
        "xml2js": "^0.4.22"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/register": "^7.15.3",
        "@babel/runtime": "^7.15.3",
        "ava": "^2.4.0",
        "cross-env": "^6.0.3",
        "mock-require": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "sinon": "^7.5.0",
        "snazzy": "^8.0.0",
        "standard": "^14.3.1"
    },
    "engines": {
        "node": ">= 0.10.0"
    },
    "preferGlobal": "true",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
